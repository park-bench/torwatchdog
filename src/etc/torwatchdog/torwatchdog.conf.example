# torwatchdog.conf.example
# Example configuration for torwatchdog.

[General]

# Logging directory
log_dir=/var/log/torwatchdog

# Log level
log_level=info

# Tor cache directory
tor_data_dir=/var/cache/torwatchdog/tor

# The URL that torwatchdog checks
url=

# Tor will listen on this port. (Default randomly chosen by developer.)
socks_port=6613

# Average delay between checks, in seconds
avg_delay=300

# Subject line, an arbitary string. Subject lines are NOT encrypted
subject=
